class Solution {
    public ListNode reverseList(ListNode head) {
        
       ListNode current = head;
       ListNode prev = null;
       ListNode next;
       
        while(current!=null){
            
            next = current.next;
            current.next = prev;
            if(next ==null)
                break;
            prev = current;
            current = next;
        }
        
        return current;
    }
}
